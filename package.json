{
  "name": "monad",
  "version": "0.0.1",
  "description": "Reusable Vue 2.x components based on TailwindCSS, accessible via StorybookJS",
  "author": "arjanski",
  "private": true,
  "scripts": {
    "dev": "nuxt --port 2000",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest",
    "storybook": "npx tailwind build assets/css/tailwind.css -o assets/css/main.css && start-storybook -p 2001"
  },
  "dependencies": {
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-knobs": "^5.3.12",
    "core-js": "2.6.10",
    "nuxt": "^2.11.0"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^1.3.2",
    "@storybook/addon-a11y": "^5.3.12",
    "@storybook/addon-backgrounds": "^5.3.12",
    "@storybook/addon-docs": "^5.3.12",
    "@storybook/addon-viewport": "^5.3.12",
    "@storybook/vue": "^5.3.12",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^24.1.0",
    "babel-preset-vue": "^2.0.2",
    "css-loader": "^3.2.0",
    "jest": "^24.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "vue-jest": "^4.0.0-0"
  }
}
